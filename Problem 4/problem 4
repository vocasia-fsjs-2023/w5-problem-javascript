// Menggunakan Math.random() untuk menghasilkan angka acak antara 1 dan 5 
const randomNumber = Math.floor(Math.random() * 5) + 1;
let message;
switch (randomNumber) {
  case 1:
    message = "coba lagi ya";
    break;
  case 2:
    message = "selamat kamu mendapatkan kupon sebanyak 5";
    break;
  case 3:
    message = "selamat kamu mendapatkan kupon sebanyak 15";
    break;
  case 4:
    message = "selamat kamu mendapatkan kupon sebanyak 50";
    break;
  case 5:
    message = "WOW, kamu menang jackpot! Selamat!!";
    break;
  default:
    message = "Angka acak tidak valid";
}

console.log(message);
